<html>
<head>
<title>National Football League General Database</title>
<link href="style.css" rel="stylesheet" type="text/css" />
<script language="JavaScript"></script>
</head>

<body>

<h2>Positions</h2>

<div id="browse">
  <table border="1" cellpadding="5">
    <thead>
      <tr>
        {{#each data.[0]}}
        <th>
          {{@key}}
        </th>
        {{/each}}
      </tr>
    </thead>
    <tbody>
      {{#each data}}
      <tr>
        <td>{{this.positionID}}</td>
        <td>{{this.description}}</td>
      </tr>
      {{/each}}
    </tbody>
  </table>
</div>

<p>&nbsp;</p>
</div> <!-- browse -->

</body>
</html>
